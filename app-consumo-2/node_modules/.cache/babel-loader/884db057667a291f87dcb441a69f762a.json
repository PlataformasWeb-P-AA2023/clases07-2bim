    if (confirm('Eliminar ' + e.telefono)) {\n                axios.delete(e.url)\n                    .then( response => {\n                        this.all();\n                    });\n            }\n        },\n        all: function () {\n            axios.get('http://127.0.0.1:8000/api/numerost/')\n                .then( response => {\n                    this.telefonos = response.data\n                });\n        }\n    },\n}\n</script>\n"],"sourceRoot":"src/components"}]}